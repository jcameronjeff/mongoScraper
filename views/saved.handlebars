<header>
  {{> _navbar}}
</header>



<div class="card-columns">


  {{#if article}}
    {{#each article}}

      <div class="card mb-4">
          <div class="view overlay">
            <a href="/details/{{this.id}}"><img src="{{this.link}}" class='img-fluid dress-preview' href = data-id= '{{this.id}}'alt='dress' /></a></div>
            <div class='mask rgba-white-slight waves-effect waves-light'>
            <a class='btn-floating btn-action ml-auto mr-4 mdb-color lighten-3 waves-effect waves-light right delete-dress' data-id="{{this._id}}">
            <i class='fa fa-chevron-right pl-1'></i></a>
            <div class='card-body'>
            <h4 class='card-title'>"{{this._id}}"</h4>
            <hr>
            <p class='font-small grey-dark-text mb-0'>"{{this.title}}"</p>
            </div>
          </div>
        </div>


    {{/each}}

    {{else}}
      <div class="container">
        <h1>NO SAVED ARTICLES</h1>
      </div>
  {{/if}}
</div>
